{
  "name": "vscode-tsserver-tracer",
  "displayName": "tsserver tracer",
  "description": "Take a trace of the TypeScript server without restarting it.",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ypresto/vscode-tsserver-tracer.git"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-tsserver-tracer.startTracing",
        "title": "TypeScript: Start tracing in tsserver"
      },
      {
        "command": "vscode-tsserver-tracer.stopTracing",
        "title": "TypeScript: Stop tracing in tsserver"
      }
    ],
    "typescriptServerPlugins": [
      {
        "name": "tsserver-tracer-plugin",
        "enableForWorkspaceTypeScriptVersions": true
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && cd plugin && npm run build",
    "compile": "node scripts/bundle-esm.js && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "esbuild": "^0.21.1",
    "esbuild-node-externals": "^1.13.1",
    "eslint": "^8.57.0",
    "open": "^10.1.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "tsserver-tracer-plugin": "file:plugin"
  }
}
